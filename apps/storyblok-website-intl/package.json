{
  "name": "storyblok-website-intl",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "Eric Reinheart",
    "email": "eric@ericr.dev"
  },
  "license": "MIT",
  "scripts": {
    "predev": "pnpm storyblok:generate:types",
    "dev": "next dev --turbopack",
    "prebuild": "pnpm storyblok:generate:types",
    "build": "next build",
    "start": "next start",
    "storyblok:generate:types": "pnpm storyblok:login && npx storyblok@3.36.1 pull-components --space 294149 --resolve-datasources && npx storyblok@3.36.1 generate-typescript-typedefs --sourceFilePaths ./components.294149.json --destinationFilePath ./src/lib/storyblok.types.ts --JSONSchemaToTSOptionsPath ./node_modules/@repo/storyblok/config/JSONSchemaToTSOptions.json",
    "storyblok:login": "storyblok-login",
    "lint": "next lint"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.8.1",
    "@headlessui/react": "^2.1.5",
    "@repo/storyblok": "workspace:*",
    "@repo/ui": "workspace:*",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.19",
    "framer-motion": "^11.2.11",
    "negotiator": "^1.0.0",
    "next": "16.1.5",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "slugify": "^1.6.6",
    "storyblok": "3.36.1"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/tailwind-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@tailwindcss/postcss": "^4.0.17",
    "@types/negotiator": "^0.6.4",
    "@types/node": "^25.2.2",
    "@types/react": "19.2.9",
    "@types/react-dom": "19.2.3",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "postcss": "^8.4.44",
    "tailwindcss": "^4.0.17",
    "typescript": "^5.9.3"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.2.9",
      "@types/react-dom": "19.2.3"
    }
  }
}
